<launch>
<!-- Vehicle limits (meters/second) -->
    <param name="max_vel_vert_abs" type="double" value="5.0" />
    <param name="max_vel_horz_abs" type="double" value="5.0" />
    <param name="max_yaw_rate_degree" type="double" value="20.0" />

<!-- Gimbal limits (degrees) -->
    <param name="gimbal_front_center_max_pitch" type="double" value="5.0" />
    <param name="gimbal_front_center_min_pitch" type="double" value="5.0" />
    <param name="gimbal_front_center_max_yaw" type="double" value="5.0" />
    <param name="gimbal_front_center_min_yaw" type="double" value="5.0" />
    <param name="gimbal_front_center_max_roll" type="double" value="5.0" />
    <param name="gimbal_front_center_min_roll" type="double" value="5.0" />
</launch>